EPOCH_TO_ITIME.OBJ : SRC_DIR:EPOCH_TO_ITIME.FOR,-
INC_DIR:EDB.INC,INC_DIR:EDBDEF.INC,INC_DIR:EDBVARS.INC
	FORTRAN $(FORFLAGS) SRC_DIR:EPOCH_TO_ITIME
WIND_RESET_HK.OBJ : SRC_DIR:WIND_RESET_HK.FOR,-
INC_DIR:EDB.INC,INC_DIR:EDBDEF.INC,INC_DIR:EDBVARS.INC
	FORTRAN $(FORFLAGS) SRC_DIR:WIND_RESET_HK
WIND_DPU_OFF.OBJ : SRC_DIR:WIND_DPU_OFF.FOR,-
INC_DIR:EDB.INC,INC_DIR:EDBDEF.INC,INC_DIR:EDBVARS.INC
	FORTRAN $(FORFLAGS) SRC_DIR:WIND_DPU_OFF
WIND_POST_PROCESS.OBJ : SRC_DIR:WIND_POST_PROCESS.FOR,-
INC_DIR:EDB.INC,INC_DIR:EDBDEF.INC,INC_DIR:EDBVARS.INC,-
INC_DIR:L0_DATA_RECORD.INC
	FORTRAN $(FORFLAGS) SRC_DIR:WIND_POST_PROCESS
WIND_MOVE_DOWN_L1.OBJ : WIND_MOVE_DOWN_L1.FOR,-
INC_DIR:EDBDEF.INC,INC_DIR:EDBVARS.INC,INC_DIR:L0_DATA_RECORD.INC
