	   1438             CALL CDF_VAR_GET(OR_ID,VAR_NUM_GCI_SUN_VECTOR,REC_NUM,INDICES,GCI_SUN_VECTOR(REC_NUM).X,STATUS)
	   1439             IF (STATUS .NE. CDF_OK) RETURN
	   1440 C
	   1441             INDICES(1) = 2
	   1442             CALL CDF_VAR_GET(OR_ID,VAR_NUM_GCI_POS,REC_NUM,INDICES,GCI_POS(REC_NUM).Y,STATUS)
	   1443             IF (STATUS .NE. CDF_OK) RETURN

