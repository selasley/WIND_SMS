	   1459             IF (STATUS .NE. CDF_OK) RETURN
	   1460             CALL CDF_VAR_GET(OR_ID,VAR_NUM_GCI_VEL,REC_NUM,INDICES,GCI_VEL(REC_NUM).Z,STATUS)
	   1461             IF (STATUS .NE. CDF_OK) RETURN
	   1462             CALL CDF_VAR_GET(OR_ID,VAR_NUM_GSE_POS,REC_NUM,INDICES,GSE_POS(REC_NUM).Z,STATUS)
