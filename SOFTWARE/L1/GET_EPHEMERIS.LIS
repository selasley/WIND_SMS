	   1359      &            VAR_NUM_GSM_POS,VAR_NUM_GSM_VEL,VAR_NUM_GCI_R_ASC,VAR_NUM_GCI_DECL,VAR_NUM_GSE_R_ASC,
	   1360      &            VAR_NUM_GSE_DECL,VAR_NUM_GSM_R_ASC,VAR_NUM_GSM_DECL,VAR_NUM_GCI_SUN_VECTOR,VAR_NUM_SPIN_RATE
	   1361         CHARACTER OR_NAME*(*),AT_NAME*(*)
	   1362 	LOGICAL GET_OR,GET_AT
	   1363 C
 C	   1364 D	type *,or_name
 C	   1365 D	type *,at_name
	   1366 	GET_OR = OR_NAME .NE. ' '
	   1367 	GET_AT = AT_NAME .NE. ' '
	   1368 	IF (.NOT. GET_OR .AND. .NOT. GET_AT) RETURN
	   1369 C
	   1370 C Open CDF ORbit file
	   1371 C
	   1372 	IF (GET_OR) THEN
	   1373           CALL CDF_OPEN(OR_NAME,OR_ID,STATUS)
	   1374           IF (STATUS .NE. CDF_OK) RETURN
	   1375 	ENDIF
	   1376 C
	   1377 C Open CDF ATtitude file
	   1378 C
	   1379 	IF (GET_AT) THEN
	   1380           CALL CDF_OPEN(AT_NAME,AT_ID,STATUS)
	   1381           IF (STATUS .NE. CDF_OK) RETURN
	   1382 	ENDIF
	   1383 C
	   1384 C Find number of record; assume same number in both files
	   1385 C
	   1386 	IF (GET_OR) THEN

