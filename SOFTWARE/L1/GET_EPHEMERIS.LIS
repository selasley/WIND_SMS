	   1431             IF (STATUS .NE. CDF_OK) RETURN
	   1432             CALL CDF_VAR_GET(OR_ID,VAR_NUM_GSE_VEL,REC_NUM,INDICES,GSE_VEL(REC_NUM).X,STATUS)
	   1433             IF (STATUS .NE. CDF_OK) RETURN
