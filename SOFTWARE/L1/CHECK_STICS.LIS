	   1598       SUBROUTINE DISSECT_PHA(PHA,STOP_ID,START_ID,ENERGY,SECTOR,SSD_ID,TIME_OF_FLIGHT)
	   1599 C
	   1600 C This subroutine does bit computations to get Stop ID, Start ID, Energy, Sector, SSD ID,
	   1601 C and Time of flight for STICS PHA words on WIND
	   1602 C
	   1603       INTEGER*4 STOP_ID,START_ID,ENERGY,SECTOR,SSD_ID,TIME_OF_FLIGHT
	   1604       INTEGER*4 ITEMP
	   1605       BYTE PHA(4)
	   1606 C
	   1607 C      WRITE(50,101) PHA
	   1608 C101   FORMAT(4(1X,Z2.2))
	   1609 C
	   1610       STOP_ID = 0
	   1611       ITEMP = PHA(1)
	   1612       CALL MVBITS( ITEMP, 6, 2, STOP_ID, 0 )
	   1613 C
	   1614       START_ID = 0
	   1615       CALL MVBITS( ITEMP, 1, 5, START_ID, 0 )
	   1616 C
	   1617       ENERGY = ISHFT( IAND( ITEMP, 1 ), 8 )
	   1618       ITEMP = PHA(2)
	   1619       ENERGY = ENERGY + IAND( ITEMP, 255 )
	   1620 C
	   1621       SECTOR = 0
	   1622       ITEMP = PHA(3)
	   1623       CALL MVBITS( ITEMP, 4, 4, SECTOR, 0 )
	   1624 C
	   1625       SSD_ID = 0
	   1626       CALL MVBITS( ITEMP, 2, 2, SSD_ID, 0 )
	   1627 C
	   1628       TIME_OF_FLIGHT = ISHFT( IAND( ITEMP, 3 ), 8 )
	   1629       ITEMP = PHA(4)
	   1630       TIME_OF_FLIGHT = TIME_OF_FLIGHT + IAND( ITEMP, 255 )
	   1631 C
