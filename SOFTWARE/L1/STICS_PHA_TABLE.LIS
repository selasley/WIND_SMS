	   1736       SUBROUTINE DISSECT_PHA(PHA,STOP_ID,START_ID,ENERGY,SECTOR,SSD_ID,TIME_OF_FLIGHT)
	   1737 C
	   1738 C This subroutine does bit computations to get Stop ID, Start ID, Energy, Sector, SSD ID,
	   1739 C and Time of flight for STICS PHA words on WIND
	   1740 C
	   1741       INTEGER*4 STOP_ID,START_ID,ENERGY,SECTOR,SSD_ID,TIME_OF_FLIGHT
	   1742       INTEGER*4 ITEMP
	   1743       BYTE PHA(4)
	   1744 C
	   1745 C      WRITE(50,101) PHA
	   1746 C101   FORMAT(4(1X,Z2.2))
	   1747 C
	   1748       STOP_ID = 0
	   1749       ITEMP = PHA(1)
	   1750       CALL MVBITS( ITEMP, 6, 2, STOP_ID, 0 )
	   1751 C
	   1752       START_ID = 0
	   1753       CALL MVBITS( ITEMP, 1, 5, START_ID, 0 )
	   1754 C
	   1755       ENERGY = ISHFT( IAND( ITEMP, 1 ), 8 )
	   1756       ITEMP = PHA(2)
	   1757       ENERGY = ENERGY + IAND( ITEMP, 255 )
	   1758 C
	   1759       SECTOR = 0
	   1760       ITEMP = PHA(3)
	   1761       CALL MVBITS( ITEMP, 4, 4, SECTOR, 0 )
	   1762 C
	   1763       SSD_ID = 0
	   1764       CALL MVBITS( ITEMP, 2, 2, SSD_ID, 0 )
	   1765 C
	   1766       TIME_OF_FLIGHT = ISHFT( IAND( ITEMP, 3 ), 8 )
	   1767       ITEMP = PHA(4)
	   1768       TIME_OF_FLIGHT = TIME_OF_FLIGHT + IAND( ITEMP, 255 )
	   1769 C
	   1770       RETURN
	   1771       END

