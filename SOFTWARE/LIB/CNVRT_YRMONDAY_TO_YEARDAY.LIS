	      1       subroutine CNVRT_YRMONDAY_TO_YEARDAY( iyr, imo, iday, iday_yr )
	      2 c     subroutine to convert from year, month, day to year day of year.
	      3 c
	      4 C      4-APR-94    H.BROER   enhanced to conform with the Gregorian calender
	      5 c
	      6       IMPLICIT NONE
	      7 
	      8       INTEGER*4	IYR,		! INPUT:  year
	      9      :		IMO,		!	  month
	     10      :		IDAY,		!	  day of month
	     11      :		IDAY_YR,	! OUTPUT: day of year
	     12      :        	IMOL(12),	! internal array
	     13      :		IMONL(12)	! internal array
	     14 
	     15       LOGICAL*1	IYRL
	     16 C
	     17       data imol/ 0, 31, 60, 91, 121, 152, 182, 213, 244, 274, 305, 335 /
	     18       data imonl/0, 31, 59, 90, 120, 151, 181, 212, 243, 273, 304, 334 /
	     19 C
	     20       iyrl = .FALSE.
	     21       IF( MOD( iyr, 4 ) .EQ. 0 )   THEN
	     22           IF( MOD( iyr, 100 ) .NE. 0 )   THEN
	     23               iyrl = .TRUE.
	     24           ELSE
	     25               IF( MOD( iyr, 400 ) .EQ. 0 )	iyrl = .TRUE.
	     26           END IF
	     27       END IF
	     28 
	     29       if( iyrl ) then
	     30           iday_yr	= imol(  imo ) + iday
	     31       else
	     32           iday_yr	= imonl( imo ) + iday
	     33       end if
	     34 
	     35 c      write( 6, * )	iyr, imo, iday, iday_yr
	     36 
	     37       return
	     38 
	     39       end

