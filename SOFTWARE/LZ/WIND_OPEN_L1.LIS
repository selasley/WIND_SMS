	   2662 
	   2663       CHARACTER*(*)	FIL1NAME
	   2664       CHARACTER*3	CTYPE
	   2665       INTEGER*4		STATUS
	   2666 
	   2667       INTEGER NBLOCKS,EXTBLOCKS,RECORD_SIZE
	   2668       INTEGER FORTERR,RMSSTC,RMSSTV,LUN,VAXERR
	   2669 C
	   2670 C   Open old or new file
	   2671 C
	   2672       RECORD_SIZE = 32766		! Maximum record length
	   2673       NBLOCKS = 20000			! Initial size for new files
	   2674       EXTBLOCKS = 200			! Extend file size for new files
	   2675       IF (NRT_MODE) THEN
	   2676         NBLOCKS = 2000			! Initial size for new NRT files
	   2677         EXTBLOCKS = 50			! Extend file size for new NRT files
	   2678       ENDIF
	   2679 C
	   2680 C Check cTYPE and do different OPEN command
	   2681 C
	   2682       IF ((CTYPE .EQ. 'OLD') .OR. (CTYPE  .EQ. 'old'))    THEN
	   2683         OPEN ( UNIT	= L1_LUN,
	   2684      +	       NAME	= FIL1NAME,
	   2685      +	       CARRIAGECONTROL='NONE',
	   2686      +	       FORM	= 'FORMATTED',
	   2687      +	       READONLY,
	   2688      +	       STATUS	= CTYPE,
	   2689      +	       RECL	= RECORD_SIZE,
	   2690      +	       ERR	= 2000,			! Return if error
	   2691      +	       IOSTAT	= STATUS)
	   2692        ELSE
	   2693          OPEN ( UNIT	= L1_Lun,
	   2694      +		NAME	= FIL1NAME,
	   2695      +		CARRIAGECONTROL='NONE',
	   2696      +		FORM	= 'FORMATTED',
	   2697      +		STATUS	= cTYPE,
	   2698      +		RECL	= Record_Size,
	   2699      +          INITIALSIZE = NBLOCKS,
	   2700      +          EXTENDSIZE = EXTBLOCKS,
	   2701      +		ERR	= 1900,			! Return if error
	   2702      +		IOSTAT	= STATUS)
	   2703        ENDIF
	   2704 1200   IF (STATUS .EQ. 0)	STATUS = SS$_NORMAL
	   2705 C
	   2706 C If this is a newly-opened file write file header into it
	   2707 C
	   2708       IF ((CTYPE .EQ. 'NEW') .OR. (CTYPE  .EQ. 'new')) THEN
	   2709           CALL WIND_WRITE_L1_HEADER(L1_LUN,1)
	   2710       ENDIF
	   2711 C
	   2712 C Done
	   2713 C
	   2714       RETURN

