1.1                                                             15-FEB-1995 12:28:40    [WIND.SOFTWARE.LZ]WIND_POST_PROCESS.FOR;9

	    909 	INCLUDE   'INC_DIR:WIND_Lun.INC/NOLIST'
	    939 C
	    940 	INTEGER SPIN
	    941 C
	    942 C Move the data.  Contents of 2nd science record --> 1st science record.
	    943 C Contents of 3rd science record --> 2nd science record.  New data will go into
	    944 C the 3rd science record in the future.  We also have to do the same thing with
	    945 C the HK's and their pointers.
	    946 C
	    947         SCI(1)              = SCI(2)
	    948         SWICS_RATE(1)       = SWICS_RATE(2)
	    949         SWICS_PHA(1)        = SWICS_PHA(2)
	    950         STICS_RATE(1)       = STICS_RATE(2)
	    951         STICS_PHA(1)        = STICS_PHA(2)
	    952         MASS_RATE(1)        = MASS_RATE(2)
	    953         MASS_PHA(1)         = MASS_PHA(2)
	    954 
	    955         HKEEP(1)            = HKEEP(2)
	    956         HKset(1)            = HKset(2)
	    957         HDBTime2( 1, 1 )    = HDBTime2( 1, 2 )
	    958         HDBTime2( 2, 1 )    = HDBTime2( 2, 2 )
	    959         HDB(1)              = HDB(2)
	    960 
	    961         SCI(2)              = SCI(3)
	    962         SWICS_RATE(2)       = SWICS_RATE(3)
	    963         SWICS_PHA(2)        = SWICS_PHA(3)
	    964         STICS_RATE(2)       = STICS_RATE(3)
	    965         STICS_PHA(2)        = STICS_PHA(3)
	    966         MASS_RATE(2)        = MASS_RATE(3)
	    967         MASS_PHA(2)         = MASS_PHA(3)
	    968 
	    969         HKEEP(2)            = HKEEP(3)
	    970         HKset(2)            = HKset(3)
	    971         HDBTime2( 1, 2 )    = HDBTime2( 1, 3 )
	    972         HDBTime2( 2, 2 )    = HDBTime2( 2, 3 )
	    973         HDB(2)              = HDB(3)
	    974 
	    975 C
	    976 C Now zero out the structures of the third Science Record to logically clear
	    977 C them.  This has to be done to allow for missing spins.
	    978 C
	    979         SCI(3).CCORE(5:) = C_BIG_NULL_BUFFER(1:LEN(SCI(3).CCORE)-4)
	    980         DO SPIN=0,59
	    981           SWICS_RATE(3).C_SW_RATE(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(SWICS_RATE(3).C_SW_RATE(SPIN))-4)
	    982           SWICS_PHA(3).C_SW_PHA(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(SWICS_PHA(3).C_SW_PHA(SPIN))-4)
	    983           STICS_RATE(3).C_ST_RATE(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(STICS_RATE(3).C_ST_RATE(SPIN))-4)
	    984           STICS_PHA(3).C_ST_PHA(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(STICS_PHA(3).C_ST_PHA(SPIN))-4)
	    985           MASS_RATE(3).C_MA_RATE(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(MASS_RATE(3).C_MA_RATE(SPIN))-4)
	    986           MASS_PHA(3).C_MA_PHA(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(MASS_PHA(3).C_MA_PHA(SPIN))-4)
	    987         ENDDO
	    988 C
	    989         HKEEP(3).CHK(5:) = C_BIG_NULL_BUFFER(1:LEN(HKEEP(3).CHK)-4)
	    990         HKset(3) = 0
	    991         HDBTime2( 1, 3 ) = 0
	    992         HDBTime2( 2, 3 ) = 0
	    993         HDB(3).CHDB(5:) = C_BIG_NULL_BUFFER(1:LEN(HDB(3).CHDB)-4)
	    994 C

