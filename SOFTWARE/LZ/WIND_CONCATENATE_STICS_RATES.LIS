	    754       INCLUDE	'INC_DIR:WIND_L0_RECORD.INC/NOLIST'
	    805       INCLUDE   'INC_DIR:WIND_Lun.INC/NOLIST'
	    835       INCLUDE	'($SSDEF)/NOLIST'
	   2260 
	   2261       INTEGER*4	II,JJ,KK,LL,SCI_REC_NO
	   2262 C
	   2263 C Loop over the 60 spins in a science record
	   2264 C
	   2265       DO 400 II = 0, 58,2
	   2266 C
	   2267 C Check to see if either of the spins that we may be about to concatenate  are
	   2268 C bad (i.e. have a quality flag which is nonzero). If both are bad, don't
	   2269 C concatenate (since we don't really know the bitrate of either). If one of the
	   2270 C two is bad, use the other's bitrate for both.
	   2271 C
	   2272         IF ((SCI(SCI_REC_NO).EDB(II).Qual.NE.0).AND.(SCI(SCI_REC_NO).EDB(II+1).Qual.NE.0))GO TO 400
	   2273         IF(SCI(SCI_REC_NO).EDB(II).Qual.NE.0)THEN
	   2274           SCI(SCI_REC_NO).EDB(II).BitRate = SCI(SCI_REC_NO).EDB(II+1).BitRate
	   2275         ELSEIF(SCI(SCI_REC_NO).EDB(II+1).Qual.NE.0)THEN
	   2276           SCI(SCI_REC_NO).EDB(II+1).BitRate = SCI(SCI_REC_NO).EDB(II).BitRate
	   2277         ENDIF
	   2278 C
	   2279 C Concatenate only if low bitrate
	   2280 C
	   2281         IF(.NOT.SCI(SCI_REC_NO).EDB(II).BitRate)THEN
	   2282           DO 200 JJ = 0,STICS_N_RATE_Size
	   2283 C
	   2284 C Copy rate data from spin II+1 to spin II, adding it to what's already there.
	   2285 C
	   2286             STICS_RATE( SCI_REC_NO ).EDB( II ).RATE( JJ + STICS_N_RATE_Size + 1 ) =
	   2287      &	    STICS_RATE( SCI_REC_NO ).EDB( II + 1 ).RATE( JJ )
	   2288 C
	   2289 C Adjust quality flags. Note that this depends on LOGICAL*1 being equivalent to BYTE
	   2290 C
	   2291 	    SCI(SCI_REC_NO).EDB(II).ST_HMR_Qual = 
	   2292      &       SCI(SCI_REC_NO).EDB(II).ST_HMR_Qual.OR.SCI(SCI_REC_NO).EDB(II+1).ST_HMR_Qual
	   2293             SCI(SCI_REC_NO).EDB(II).ST_SMR_Qual =
	   2294      &       SCI(SCI_REC_NO).EDB(II).ST_SMR_Qual.OR.SCI(SCI_REC_NO).EDB(II+1).ST_SMR_Qual
	   2295             SCI(SCI_REC_NO).EDB(II).ST_BR_Qual =
	   2296      &       SCI(SCI_REC_NO).EDB(II).ST_BR_Qual.OR.SCI(SCI_REC_NO).EDB(II+1).ST_BR_Qual
	   2297             SCI(SCI_REC_NO).EDB(II).ST_OMR_Qual =
	   2298      &       SCI(SCI_REC_NO).EDB(II).ST_OMR_Qual.OR.SCI(SCI_REC_NO).EDB(II+1).ST_OMR_Qual
	   2299             SCI(SCI_REC_NO).EDB(II).ST_ER_Qual =
	   2300      &       SCI(SCI_REC_NO).EDB(II).ST_ER_Qual.OR.SCI(SCI_REC_NO).EDB(II+1).ST_ER_Qual
	   2301 C
	   2302 C Remove the data from the odd-numbered spin.
	   2303 C
	   2304             STICS_RATE( SCI_REC_NO ).EDB( II + 1 ).RATE( JJ ) = 0
	   2305  200      CONTINUE
	   2306 C
	   2307 C Adjust quality flags. Since the odd spins no longer contain data, set them to
	   2308 C 8 ( which denotes 'missing EDB'; see EDB.INC).
	   2309 C
	   2310           SCI(SCI_REC_NO).EDB(II+1).ST_HMR_Qual = 8
	   2311           SCI(SCI_REC_NO).EDB(II+1).ST_SMR_Qual = 8
	   2312           SCI(SCI_REC_NO).EDB(II+1).ST_BR_Qual  = 8
	   2313           SCI(SCI_REC_NO).EDB(II+1).ST_OMR_Qual = 8

