    1	     70       STRUCTURE /ST_L1_FILE_HEADER/
    1	     71           UNION
    1	     72           MAP
    1	     73               CHARACTER*4     ident/'HDRF'/
    1	     74               CHARACTER*2     header_vers       ! header version
    1	     75               CHARACTER*72    input_file        ! name of input file
    1	     76               CHARACTER*2     medium            ! input medium code:
    1	     77                                                 ! 'DK' = disk
    1	     78                                                 ! 'CD' = cd-rom
    1	     79               CHARACTER*3     credate_doy       ! input file creation date, day
    1	     80                                                 ! of year 
    1	     81               CHARACTER*4     credate_year      ! input file creation date, year
    1	     82               CHARACTER*11    credate           ! input file creation date
    1	     83                                                 ! 'DD-MMM-YYYY'
    1	     84               CHARACTER*23    date_of_EXE       ! version of program which wrote
    1	     85                                                 ! the L1 file
    1	     86               CHARACTER*1     hw_type           ! system hardware type which ran
    1	     87                                                 ! program: 'V' = VAX, 'A' = AXP
    1	     88               CHARACTER*11    run_date          ! program run date 'DD-MMM-YYYY'
    1	     89               CHARACTER*20    start_time        ! user-specified start time
    1	     90                                                 ! 'DD-MMM-YYYY HH:MM:SS'
    1	     91               CHARACTER*20    stop_time         ! user-specified stop time
    1	     92                                                 ! 'DD-MMM-YYYY HH:MM:SS'
    1	     93               CHARACTER*3     comment_len       ! length of user comment
    1	     94               CHARACTER*80    comment           ! user-supplied comment
    1	     95               CHARACTER*4     L1_records        ! number of L1 records in file
    1	     96               CHARACTER*6     EDB_count         ! number of good EDBs (spins) stored
    1	     97               CHARACTER*6     EDB_lost          ! number of EDBs (spins) lost
    1	     98               CHARACTER*6     EDB_skip          ! number of EDBs (spins) skipped
    1	     99               CHARACTER*5     program_vers      ! version of WIND_CD_READ program
    1	    100               CHARACTER*1     eph_avail		! 'T' = orbit and attitude info
    1	    101 						! in sci rec hdr; 'F' = not there
    1	    102               CHARACTER*116   spare             ! makes total size 400 bytes
    1	    103           END MAP
    1	    104           MAP
    1	    105               CHARACTER*400   c_all             ! references entire structure
    1	    106           END MAP
    1	    107           END UNION
