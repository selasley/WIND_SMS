	    767       HOUR	= MILLIS / 3600000
	    768 
	    769       Time.YEAR	= YEAR
	    770       Time.DOY	= DOY
	    771       Time.HR	= HOUR
	    772       Time.MIN	= MINUTE
	    773       Time.SEC	= SECOND
	    774       Time.CSEC	= CSEC
