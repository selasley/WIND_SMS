	    770       INCLUDE 'INC_DIR:WIND_L0_RECORD.INC/NOLIST'
	    821       INCLUDE 'INC_DIR:WIND_Lun.INC/NOLIST'
	    851       INCLUDE '($SSDEF)/NOLIST'
	   2276 
	   2277       INTEGER*4	II,	  ! Spin index
	   2278      :	        JJ,	  ! Byte index
	   2279      :          KK,       ! Pointer to first good spin
	   2280      :          MRSTART,  ! Index of last Matrix Rate for spin II     
	   2281      :          MESTART,  ! Index of last Matrix Element for spin II  
	   2282      :          ERSTART,  ! Index of last Engineering Rate for spin II
	   2283      :          MRSTART2, ! Index of last Matrix Rate for spin II+2
	   2284      :          MESTART2, ! Index of last Matrix Element for spin II SCI-REC 2 or 3
	   2285      :          ERSTART2, ! Index of last Engineering Rate for spin II+2
	   2286      :          INC1,INC2,              ! (STICS) Amount to increment to reach next, 2nd spin
	   2287      :          NEXTSPIN,NEXTSPIN2,     ! Pointer to next, 2nd spin
	   2288      :          RECNO,RECNO2,           ! SCI-REC of next, 2nd spin
	   2289      :          NEXTSPIN_PHA,           ! Pointer to next spin with PHA
	   2290      :          RECNO_PHA,              ! SCI-REC of next spin
	   2291      :          ACTIVE_PHA_RANGE,       ! Active PHA range ID; 0,1, or 2
	   2292      :          APHASTART(3),           ! Start index for PHA ranges
	   2293      :          HOUSEKEEPING,           ! House keeping byte for SWICS
	   2294      :          SUM,SUM2,               ! Used as accumulators
	   2295      :          ITEMP
	   2296       INTEGER*4 INCREMENT,WIND_DECOMPRESS    ! Functions
	   2297       BYTE      WIND_COMPRESS                ! Function 
	   2298       LOGICAL   HIGHLOW,LOWHIGH,CODE_C
	   2299 C
	   2300 C In some science records, spins may be absent, fill, or otherwise of low
	   2301 C quality.  Determine first good quality spin, and use it for bit rate
	   2302 C determination.
	   2303 C
	   2304       KK=0
	   2305       DO 5 WHILE((SCI( 1 ).EDB(KK).Qual.NE.0).AND.(KK.LT.59))
	   2306         KK=KK+1
	   2307   5   CONTINUE    
	   2308 C
	   2309 C Shift SWICS data. Loop over the 60 spins in a science record. Since some
	   2310 C rates are read out 1 spin after accumulation, some are read out 2 spins, some
	   2311 C are read out 1 full science record after accumulation, and some are read out
	   2312 C 2 full science records after accumulation, we have several possible
	   2313 C conditions.
	   2314 C
	   2315       DO 200 II = 0, 59
	   2316 C
	   2317 C RECNO, and RECNO2 are the numbers of the science records containing NEXTSPIN
	   2318 C and NEXTSPIN2, respectively.
	   2319 C
	   2320         NEXTSPIN=MOD(II+1,60)
	   2321         NEXTSPIN2=MOD(II+2,60)
	   2322         RECNO=(II+1)/60+1
	   2323         RECNO2=(II+2)/60+1
	   2324 C
	   2325         IF(SCI( 1 ).EDB( II ).BitRate)THEN      		! High bit rate (current spin)
	   2326           MRSTART=80
	   2327           MESTART=101
	   2328           ERSTART=109
	   2329           LOWHIGH=.FALSE.

WIND_SHIFT_DATA                                                 30-NOV-1995 14:17:39    DEC Fortran V6.2-508                Page   4
1.8                                                              9-FEB-1995 20:43:31    [WIND.SOFTWARE.LZ]WIND_SHIFT_DATA.FOR;82

	   2330           IF(.NOT.(SCI( RECNO ).EDB( NEXTSPIN ).BitRate))THEN	! High bit rate (next spin)
	   2331             HIGHLOW=.TRUE.
	   2332           ELSE
	   2333             HIGHLOW=.FALSE.
	   2334           ENDIF
	   2335         ELSE                                                    ! Low bit rate (current spin)
	   2336           MRSTART=26
	   2337           MESTART=47
	   2338           ERSTART=55
	   2339           HIGHLOW=.FALSE.
	   2340           IF(SCI( RECNO ).EDB( NEXTSPIN ).BitRate)THEN	        ! High bit rate (next spin)
	   2341             LOWHIGH=.TRUE.
	   2342           ELSE
	   2343             LOWHIGH=.FALSE.
	   2344           ENDIF
	   2345         ENDIF
	   2346 C
	   2347         IF(SCI( RECNO2 ).EDB( NEXTSPIN2).BitRate)THEN	        ! High bit rate
	   2348           MRSTART2=80
	   2349           ERSTART2=109
	   2350         ELSE                                                    ! Low bit rate
	   2351           MRSTART2=26
	   2352           ERSTART2=55
	   2353         ENDIF
	   2354 C
	   2355 C Basic Rates (bytes 0 to MRSTART-1) are read out 1 spin after accumulation.
	   2356 C Shift them back by one spin, except in various special cases.
	   2357 C
	   2358         IF(LOWHIGH)THEN
	   2359 C
	   2360 C When the current record is in low bit rate, and the record that follows is in
	   2361 C high bit rate, we cannot simply shift the Basic Rates. The Basic Rates at low
	   2362 C bit rate are summed over several sectors, while the Basic Rates at high bit
	   2363 C rate are each specific to a sector. Thus we must sum the data together
	   2364 C appropriately before we shift. Since the SWICS rates are "compressed" (i.e.
	   2365 C logarithmically encoded so as to fit into a byte), we must decompress them, 
	   2366 C add the decompressed values and recompress them.
	   2367 C
	   2368           DO 10 JJ = 0, 15
	   2369             ITEMP = HDB(RECNO).BDH.DAT(189)
	   2370             CODE_C = BTEST(ITEMP,0)
	   2371             SUM = WIND_DECOMPRESS(SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(2*JJ),CODE_C) +
	   2372      &            WIND_DECOMPRESS(SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(2*JJ+1),CODE_C)
	   2373             SWICS_RATE( 1 ).EDB( II ).RATE(JJ) = WIND_COMPRESS(SUM,CODE_C)
	   2374   10      CONTINUE
	   2375 C
	   2376 C The active PHA range determines which data are more fully transmitted.
	   2377 C
	   2378           HOUSEKEEPING = SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(61)
	   2379           ACTIVE_PHA_RANGE = IBITS(HOUSEKEEPING,6,2)
	   2380 C
	   2381 C Based on which range is active, get indices needed for summing.
	   2382 C
	   2383           IF(ACTIVE_PHA_RANGE.EQ.0)THEN      ! Active range=2a
	   2384             APHASTART(1)=32
	   2385             APHASTART(2)=48
	   2386             APHASTART(3)=64

WIND_SHIFT_DATA                                                 30-NOV-1995 14:17:39    DEC Fortran V6.2-508                Page   5
1.8                                                              9-FEB-1995 20:43:31    [WIND.SOFTWARE.LZ]WIND_SHIFT_DATA.FOR;82

	   2387           ELSEIF(ACTIVE_PHA_RANGE.EQ.1)THEN  ! Active range=2b
	   2388             APHASTART(1)=48
	   2389             APHASTART(2)=32
	   2390             APHASTART(3)=64
	   2391           ELSE                               ! Active range=2c
	   2392             APHASTART(1)=64
	   2393             APHASTART(2)=32
	   2394             APHASTART(3)=48
	   2395           ENDIF  
	   2396 C
	   2397 C For active PHA range, sum rates in pairs and shift.
	   2398 C
	   2399           DO 20 JJ = 0, 7                 
	   2400             ITEMP = HDB(RECNO).BDH.DAT(189)
	   2401             CODE_C = BTEST(ITEMP,0)
	   2402             SUM = 
	   2403      &       WIND_DECOMPRESS(SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(2*JJ + APHASTART(1)), CODE_C) + 
	   2404      &       WIND_DECOMPRESS(SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(2*JJ + APHASTART(1) + 1),CODE_C) 
	   2405             SWICS_RATE(1).EDB(II).RATE(JJ+8) = WIND_COMPRESS(SUM,CODE_C)
	   2406   20      CONTINUE
	   2407 C
	   2408 C For other two PHA ranges, sum all sectors and shift.
	   2409 C      
	   2410           SUM=0
	   2411           SUM2=0                  
	   2412           DO 40 JJ = 0, 15
	   2413             ITEMP = HDB(RECNO).BDH.DAT(189)
	   2414             CODE_C = BTEST(ITEMP,0)
	   2415             SUM = SUM + 
	   2416      &            WIND_DECOMPRESS(SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(JJ + APHASTART(2)),CODE_C)
	   2417             SUM2= SUM2 + 
	   2418      &            WIND_DECOMPRESS(SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(JJ + APHASTART(3)),CODE_C)
	   2419   40      CONTINUE
	   2420           SWICS_RATE(1).EDB(II).RATE(24) = WIND_COMPRESS(SUM, CODE_C)
	   2421           SWICS_RATE(1).EDB(II).RATE(25) = WIND_COMPRESS(SUM2,CODE_C)
	   2422 C
	   2423 C Adjust quality flags for Basic Rates. Note that this depends on LOGICAL*1
	   2424 C being equivalent to BYTE.
	   2425 C
	   2426           SCI(1).EDB(II).SW_BR_Qual  = SCI(RECNO).EDB(NEXTSPIN).SW_BR_Qual
	   2427 C
	   2428         ELSEIF(HIGHLOW)THEN
	   2429 C
	   2430 C Specific Sector information is lost in low bit rate, as the numbers
	   2431 C transmitted are accumulated over several sectors. In high bit rate, specific
	   2432 C sector information is needed. Therefore, when a spin in high bit rate is
	   2433 C immediately followed by a spin in low bit rate, we cannot shift the basic
	   2434 C rates. Thus, we zero them and set the quality flags to indicate that the
	   2435 C Basic Rates are missing.
	   2436 C
	   2437           DO 60 JJ = 0, 15
	   2438             SWICS_RATE( 1 ).EDB( II ).RATE(JJ) = 0
	   2439   60      CONTINUE
	   2440 C
	   2441 C Set quality flags for Basic Rates to 8 (missing data). Note that this depends
	   2442 C on LOGICAL*1 being equivalent to BYTE.
	   2443 C

WIND_SHIFT_DATA                                                 30-NOV-1995 14:17:39    DEC Fortran V6.2-508                Page   6
1.8                                                              9-FEB-1995 20:43:31    [WIND.SOFTWARE.LZ]WIND_SHIFT_DATA.FOR;82

	   2444           SCI(1).EDB(II).SW_BR_Qual  = 8
	   2445         ELSE
	   2446 C
	   2447 C If the spin that follows the current one has the same bitrate as the current
	   2448 C one, then the Basic Rates may easily be shifted.
	   2449 C
	   2450           DO 110 JJ = 0, MRSTART-1
	   2451             SWICS_RATE( 1 ).EDB( II ).RATE(JJ) = SWICS_RATE(RECNO).EDB(NEXTSPIN).RATE(JJ)
	   2452  110      CONTINUE
	   2453 C
	   2454 C Adjust quality flags for Basic Rates. Note that this depends on LOGICAL*1
	   2455 C being equivalent to BYTE.
	   2456 C
	   2457           SCI(1).EDB(II).SW_BR_Qual  = SCI(RECNO).EDB(NEXTSPIN).SW_BR_Qual
	   2458         ENDIF
	   2459 C
	   2460 C PHA data is read out 1 spin after being accumulated also. Transfer SWICS PHA
	   2461 C data from spin II+1 to spin II.
	   2462 C
	   2463         SWICS_PHA( 1 ).C_SW_PHA( II ) = SWICS_PHA(RECNO).C_SW_PHA(NEXTSPIN)
	   2464 C
	   2465 C Adjust the # of PHA words for this spin to reflect the change
	   2466 C
	   2467         SCI( 1 ).EDB( II ).SWICS_Len = SCI(RECNO).EDB(NEXTSPIN).SWICS_Len
	   2468 C
	   2469 C Adjust quality flags for PHA. Note that this depends on LOGICAL*1 being
	   2470 C equivalent to BYTE.
	   2471 C
	   2472         SCI(1).EDB(II).SW_P_Qual = SCI(RECNO).EDB(NEXTSPIN).SW_P_Qual
	   2473 C     
	   2474 C Matrix Rates (bytes MRSTART to MESTART-1) are read out 2 spins after
	   2475 C accumulation. Shift them back by two spins.
	   2476 C
	   2477         DO 120 JJ = 0,20
	   2478           SWICS_RATE(1).EDB(II).RATE(JJ+MRSTART)=SWICS_RATE(RECNO2).EDB(NEXTSPIN2).RATE(JJ+MRSTART2)
	   2479  120    CONTINUE
	   2480 C
	   2481 C Adjust quality flags for Matrix Rates. Note that this depends on LOGICAL*1
	   2482 C being equivalent to BYTE.
	   2483 C
	   2484         SCI(1).EDB(II).SW_MR_Qual  = SCI(RECNO2).EDB(NEXTSPIN2).SW_MR_Qual
	   2485 C       
	   2486 C Engineering Rates (bytes MESTART+1 to ERSTART) are read out 2 spins after
	   2487 C accumulation. Shift them back by two spins.
	   2488 C
	   2489         DO 130 JJ = 0,5
	   2490           SWICS_RATE(1).EDB(II).RATE(JJ+ERSTART)=SWICS_RATE(RECNO2).EDB(NEXTSPIN2).RATE(JJ+ERSTART2)
	   2491  130    CONTINUE
	   2492 C
	   2493 C Adjust quality flags for Engineering Rates. Note that this depends on
	   2494 C LOGICAL*1 being equivalent to BYTE.
	   2495 C
	   2496         SCI(1).EDB(II).SW_ER_Qual = SCI(RECNO2).EDB(NEXTSPIN2).SW_ER_Qual
	   2497 C
	   2498 C Now shift Matrix Elements. Matrix Elements 0-121 are shifted by 1 science
	   2499 C record. Since the Matrix Elements are subcommutated over 60 spins, eight of
	   2500 C them are read out each spin (appearing in bytes MESTART to MESTART+7). Thus,

WIND_SHIFT_DATA                                                 30-NOV-1995 14:17:39    DEC Fortran V6.2-508                Page   7
1.8                                                              9-FEB-1995 20:43:31    [WIND.SOFTWARE.LZ]WIND_SHIFT_DATA.FOR;82

	   2501 C the first 15 spins hold the Matrix Elements 0-119, all of which are shifted
	   2502 C by one science record.
	   2503 C
	   2504         IF(II.LT.15)THEN
	   2505 C
	   2506           IF(SCI( 2 ).EDB( II ).BitRate)THEN		! High bit rate
	   2507             MESTART2=101
	   2508           ELSE                                          ! Low bit rate
	   2509             MESTART2=47
	   2510           ENDIF
	   2511 C     
	   2512           DO 170 JJ = 0,7
	   2513             SWICS_RATE(1).EDB(II).RATE(JJ+MESTART) = SWICS_RATE(2).EDB(II).RATE(JJ+MESTART2)
	   2514  170      CONTINUE
	   2515 C
	   2516 C Adjust quality flags for Matrix Elements 0-121. Note that this depends on
	   2517 C LOGICAL*1 being equivalent to BYTE.
	   2518 C
	   2519           SCI(1).EDB(II).SW_ME1_Qual = SCI(2).EDB(II).SW_ME1_Qual
	   2520 C
	   2521 C Spin 15 is an unusual one. Bytes MESTART and MESTART+1 hold Matrix Elements
	   2522 C 120, and 121, which are shifted by one science record, while bytes MESTART+2
	   2523 C to MESTART+7 hold Matrix Elements 122-127, which are shifted by two science
	   2524 C records.
	   2525 C
	   2526         ELSEIF(II.EQ.15)THEN
	   2527 C
	   2528           IF(SCI( 2 ).EDB( 15 ).BitRate)THEN		! High bit rate
	   2529             MESTART2=101
	   2530           ELSE                                          ! Low bit rate
	   2531             MESTART2=47
	   2532           ENDIF
	   2533 C
	   2534           SWICS_RATE(1).EDB(15).RATE(MESTART+1) = SWICS_RATE(2).EDB(15).RATE(MESTART2+1)
	   2535           SWICS_RATE(1).EDB(15).RATE(MESTART+2) = SWICS_RATE(2).EDB(15).RATE(MESTART2+2)
	   2536 C
	   2537           IF(SCI( 3 ).EDB( 15 ).BitRate)THEN		! High bit rate
	   2538             MESTART2=101
	   2539           ELSE                                          ! Low bit rate
	   2540             MESTART2=47
	   2541           ENDIF
	   2542 C
	   2543           DO 180 JJ = 2,7
	   2544             SWICS_RATE(1).EDB(15).RATE(JJ+MESTART) = SWICS_RATE(3).EDB(15).RATE(JJ+MESTART2)
	   2545  180      CONTINUE
	   2546         ELSE
	   2547 C
	   2548 C The remaining spins (16-59) hold Matrix Elements 128-479, which are all
	   2549 C shifted by 2 science records.
	   2550 C
	   2551           IF(SCI( 3 ).EDB( II ).BitRate)THEN		! High bit rate
	   2552             MESTART2=101
	   2553           ELSE                                          ! Low bit rate
	   2554             MESTART2=47
	   2555           ENDIF
	   2556 C
	   2557           DO 190 JJ = 0,7

WIND_SHIFT_DATA                                                 30-NOV-1995 14:17:39    DEC Fortran V6.2-508                Page   8
1.8                                                              9-FEB-1995 20:43:31    [WIND.SOFTWARE.LZ]WIND_SHIFT_DATA.FOR;82

	   2558             SWICS_RATE(1).EDB(II).RATE(JJ+MESTART) = SWICS_RATE(3).EDB(II).RATE(JJ+MESTART2)
	   2559  190      CONTINUE
	   2560 C
	   2561 C Adjust quality flags for Matrix Elements 122-479. Note that this depends on
	   2562 C LOGICAL*1 being equivalent to BYTE.
	   2563 C
	   2564           SCI(1).EDB(II).SW_ME2_Qual = SCI(3).EDB(II).SW_ME2_Qual
	   2565         ENDIF
	   2566 200   CONTINUE
	   2567 C
	   2568 C Shift MASS data. Loop over the 60 spins in a science record. Since some rates
	   2569 C are read out 1 spin after accumulation, and some are read out 2 spins after
	   2570 C accumulation, we have several possible conditions.
	   2571 C
	   2572       DO 300 II = 0, 59
	   2573         IF(II.LT.59)THEN
	   2574 C
	   2575 C Sectored Engineering Rates (bytes 4-10) and Basic Rates (bytes 11-14) are
	   2576 C read out 1 spin after accumulation. Shift them back by one spin.
	   2577 C
	   2578           DO 210 JJ = 4, 14
	   2579             MASS_RATE( 1 ).EDB( II ).RATE(JJ) = MASS_RATE( 1 ).EDB( II + 1 ).RATE(JJ)
	   2580  210      CONTINUE
	   2581 C
	   2582 C Adjust quality flags for Sectored Engineering Rates and for Basic Rates. Note
	   2583 C that this depends on LOGICAL*1 being equivalent to BYTE.
	   2584 C
	   2585           SCI(1).EDB(II).MA_SER_Qual = SCI(1).EDB(II+1).MA_SER_Qual
	   2586           SCI(1).EDB(II).MA_BR_Qual  = SCI(1).EDB(II+1).MA_BR_Qual
	   2587 C
	   2588 C PHA data is read out 1 spin after being accumulated also. Transfer MASS PHA
	   2589 C data from spin II+1 to spin II.
	   2590 C
	   2591           MASS_PHA( 1 ).C_MA_PHA( II ) = MASS_PHA( 1 ).C_MA_PHA( II + 1 )
	   2592 C
	   2593 C Adjust the # of PHA words for this spin to reflect the change
	   2594 C
	   2595           SCI( 1 ).EDB( II ).MASS_Len = SCI( 1 ).EDB( II + 1 ).MASS_Len
	   2596 C
	   2597 C Adjust quality flags for PHA. Note that this depends on LOGICAL*1 being
	   2598 C equivalent to BYTE.
	   2599 C
	   2600           SCI(1).EDB(II).MA_P_Qual = SCI(1).EDB(II+1).MA_P_Qual
	   2601         ENDIF
	   2602 C
	   2603         IF(II.LT.58)THEN
	   2604 C
	   2605 C Unsectored Engineering Rates (bytes 0-3) are read out 2 spins after
	   2606 C accumulation.  Shift them back by two spins.
	   2607 C
	   2608           DO 220 JJ = 0, 3
	   2609             MASS_RATE( 1 ).EDB( II ).RATE(JJ) = MASS_RATE( 1 ).EDB( II + 2 ).RATE(JJ)
	   2610  220      CONTINUE
	   2611 C
	   2612 C Adjust quality flags for Unsectored Engineering Rates. Note that this depends
	   2613 C on LOGICAL*1 being equivalent to BYTE.
	   2614 C

WIND_SHIFT_DATA                                                 30-NOV-1995 14:17:39    DEC Fortran V6.2-508                Page   9
1.8                                                              9-FEB-1995 20:43:31    [WIND.SOFTWARE.LZ]WIND_SHIFT_DATA.FOR;82

	   2615           SCI(1).EDB(II).MA_UER_Qual = SCI(1).EDB(II+2).MA_UER_Qual
	   2616 C
	   2617 C Matrix Rates(bytes 15-24) are read out 2 spins after accumulation. Shift them
	   2618 C back by two spins.
	   2619 C
	   2620           DO 230 JJ = 15,24
	   2621             MASS_RATE( 1 ).EDB( II ).RATE(JJ) = MASS_RATE( 1 ).EDB( II + 2 ).RATE(JJ)
	   2622  230      CONTINUE
	   2623 C
	   2624 C Adjust quality flags for Matrix Rates. Note that this depends on LOGICAL*1
	   2625 C being equivalent to BYTE.
	   2626 C
	   2627           SCI(1).EDB(II).MA_MR_Qual = SCI(1).EDB(II+2).MA_MR_Qual
	   2628         ENDIF
	   2629 C
	   2630         IF(II.GE.58)THEN
	   2631 C
	   2632 C When we near the boundary between science records, the process is the same,
	   2633 C but the indexing is somewhat different. As before, sectored Engineering Rates
	   2634 C (bytes 0-3) are read out 2 spins after accumulation; but now, in the process
	   2635 C of shifting them back by two spins, we reach into the next science record.
	   2636 C
	   2637           DO 250 JJ = 0, 3
	   2638             MASS_RATE( 1 ).EDB( II ).RATE(JJ) = MASS_RATE( 2 ).EDB( II-58 ).RATE(JJ)
	   2639  250      CONTINUE
	   2640 C
