	    924 	INCLUDE   'INC_DIR:WIND_Lun.INC/NOLIST'
	    954 C
	    955 	INTEGER SPIN
	    956 C
	    957 C Move the data.  Contents of 2nd science record --> 1st science record.
	    958 C Contents of 3rd science record --> 2nd science record.  New data will go into
	    959 C the 3rd science record in the future.  We also have to do the same thing with
	    960 C the HK's and their pointers.
	    961 C
	    962         SCI(1)              = SCI(2)
	    963         SWICS_RATE(1)       = SWICS_RATE(2)
	    964         SWICS_PHA(1)        = SWICS_PHA(2)
	    965         STICS_RATE(1)       = STICS_RATE(2)
	    966         STICS_PHA(1)        = STICS_PHA(2)
	    967         MASS_RATE(1)        = MASS_RATE(2)
	    968         MASS_PHA(1)         = MASS_PHA(2)
	    969 
	    970         HKEEP(1)            = HKEEP(2)
	    971         HKset(1)            = HKset(2)
	    972         HDBTime2( 1, 1 )    = HDBTime2( 1, 2 )
	    973         HDBTime2( 2, 1 )    = HDBTime2( 2, 2 )
	    974         HDB(1)              = HDB(2)
	    975 
	    976         SCI(2)              = SCI(3)
	    977         SWICS_RATE(2)       = SWICS_RATE(3)
	    978         SWICS_PHA(2)        = SWICS_PHA(3)
	    979         STICS_RATE(2)       = STICS_RATE(3)
	    980         STICS_PHA(2)        = STICS_PHA(3)
	    981         MASS_RATE(2)        = MASS_RATE(3)
	    982         MASS_PHA(2)         = MASS_PHA(3)
	    983 
	    984         HKEEP(2)            = HKEEP(3)
	    985         HKset(2)            = HKset(3)
	    986         HDBTime2( 1, 2 )    = HDBTime2( 1, 3 )
	    987         HDBTime2( 2, 2 )    = HDBTime2( 2, 3 )
	    988         HDB(2)              = HDB(3)
	    989 
	    990 C
	    991 C Now zero out the structures of the third Science Record to logically clear
	    992 C them.  This has to be done to allow for missing spins.
	    993 C
	    994         SCI(3).CCORE(5:) = C_BIG_NULL_BUFFER(1:LEN(SCI(3).CCORE)-4)
	    995         DO SPIN=0,59
	    996           SWICS_RATE(3).C_SW_RATE(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(SWICS_RATE(3).C_SW_RATE(SPIN))-4)
	    997           SWICS_PHA(3).C_SW_PHA(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(SWICS_PHA(3).C_SW_PHA(SPIN))-4)
	    998           STICS_RATE(3).C_ST_RATE(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(STICS_RATE(3).C_ST_RATE(SPIN))-4)
	    999           STICS_PHA(3).C_ST_PHA(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(STICS_PHA(3).C_ST_PHA(SPIN))-4)
	   1000           MASS_RATE(3).C_MA_RATE(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(MASS_RATE(3).C_MA_RATE(SPIN))-4)
	   1001           MASS_PHA(3).C_MA_PHA(SPIN)(5:) = C_BIG_NULL_BUFFER(1:LEN(MASS_PHA(3).C_MA_PHA(SPIN))-4)
	   1002         ENDDO
	   1003 C
	   1004         HKEEP(3).CHK(5:) = C_BIG_NULL_BUFFER(1:LEN(HKEEP(3).CHK)-4)
	   1005         HKset(3) = 0
	   1006         HDBTime2( 1, 3 ) = 0
	   1007         HDBTime2( 2, 3 ) = 0
	   1008         HDB(3).CHDB(5:) = C_BIG_NULL_BUFFER(1:LEN(HDB(3).CHDB)-4)
	   1009 C

