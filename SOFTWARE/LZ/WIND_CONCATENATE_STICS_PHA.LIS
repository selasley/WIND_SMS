	   2316      &        (SCI(SCI_REC_NO).EDB(II).ST_P_Qual.NE.0).AND.(SCI(SCI_REC_NO).EDB(II+1).ST_P_Qual.EQ.0))THEN
	   2317               SCI(SCI_REC_NO).EDB(II).ST_P_Qual = IBSET( SCI(SCI_REC_NO).EDB(II).ST_P_Qual, 4 )
	   2318             ENDIF
	   2319 	    SCI(SCI_REC_NO).EDB(II).ST_P_Qual =
	   2320      &       SCI(SCI_REC_NO).EDB(II).ST_P_Qual.OR.SCI(SCI_REC_NO).EDB(II+1).ST_P_Qual
	   2321 C
	   2322 C Remove data from odd-numbered spin
	   2323 C
	   2324             DO 300 LL=0,(SCI( SCI_REC_NO ).EDB( II + 1 ).STICS_Len-1)*4
	   2325               STICS_PHA( SCI_REC_NO ).EDB( II + 1 ).PHA(LL) = 0
	   2326  300        CONTINUE
	   2327 C
	   2328 C Adjust the # of PHA words for this spin to reflect the change
	   2329 C
	   2330             SCI( SCI_REC_NO ).EDB( II + 1 ).STICS_Len = 0
	   2331 C
	   2332 C Adjust quality flags. Since the odd spins no longer contain data, set them to
	   2333 C 8 ( which denotes 'missing EDB'; see EDB.INC).
	   2334 C
	   2335 	    SCI(SCI_REC_NO).EDB(II + 1).ST_P_Qual = 8
	   2336           ENDIF
	   2337 C
	   2338         ENDIF
	   2339  400  CONTINUE
	   2340 C
	   2341       RETURN
	   2342 C
	   2343       END

