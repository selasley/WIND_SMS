	   2539      .			      J,J+1,BASIC_RATE(I,(J+2)/2,K),(BASIC_PHA(I,J,K) + BASIC_PHA(I,J+1,K))
	   2540  250		   FORMAT(1X,I4.3,3(':',I2.2),3X,'Sectors',I2,'+',I2,'  Basic Rate = ',F7.0,3X,
	   2541      .                       'Basic PHA = ', F7.0)
	   2542 	         ELSE
	   2543 		   GOOD_BR = GOOD_BR + 1
	   2544 	         ENDIF
	   2545 	      ELSE
	   2546 	         BASIC_W(I,J,K) = 1.0
	   2547 	      ENDIF
	   2548               BASIC_W(I,J+1,K) = BASIC_W(I,J,K)
	   2549 	    END DO
	   2550        	  END DO
	   2551         END DO
	   2552 C
	   2553 C Do Basic Rate normalization and efficiency correction. H+, He+2 and He+ are all Basic Rate 2 
	   2554 C
	   2555         DO I = 0, 31
	   2556           IF(DVS_HITS(I).EQ.0) DVS_HITS(I)=1.0
	   2557 	  DO J = 0,15
	   2558 	    DO K = 1,3
	   2559 	      DO L = 8,9
	   2560                 CORR_DATA(I,J,K,L) = RAW_COUNTS(I,J,K,L) * BASIC_W(I,J,L-8) * DEADTIME_CORR(J)
	   2561      .				   /(EFF_CORR(I,L) * DVS_HITS(I) * GEO_FAC * DELTA_T)
	   2562 	      END DO
