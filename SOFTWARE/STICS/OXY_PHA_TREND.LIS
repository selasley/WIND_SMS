	   2569 C			****************************************
	   2570 C Sum over sectors and average over voltage steps for different energy ranges.
	   2571 C
	   2572 	DO L = 8, 9
	   2573 	  DO I = 0,31
	   2574 	    DO K = 1, 3
	   2575 	      DO J = 0, 15 
	   2576                 OMNI_DATA(I,L) = OMNI_DATA(I,L) + CORR_DATA(I,J,K,L)/16.0/3.0
	   2577 		OMNI_RAW(I,L)  = OMNI_RAW(I,L) + RAW_COUNTS(I,J,K,L)
	   2578 	      END DO
	   2579 	    END DO
	   2580 	  END DO
	   2581         END DO
	   2582 C
	   2583 C Rebin data by adding adjacent voltage steps together to get larger bin 10-30 keV/amu 
	   2584 C
	   2585 
	   2586 	  DO I = DVSTEP_MIN1, DVSTEP_MAX1
	   2587 	       OXY_DATA(1) = OXY_DATA(1) + OMNI_DATA(I,8)*ENERGY_BANDWIDTH*DPU_EPQ(I)
	   2588      .						/ENERGY_BIN1 
