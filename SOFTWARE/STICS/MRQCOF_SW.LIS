	      1       SUBROUTINE MRQCOF(X,Y,SIG,NDATA,A,MA,LISTA,MFIT,ALPHA,BETA,NALP,
	      2      *CHISQ,FUNCS,max_cnt,max_loc,fwhm,he_max_cnt)
	      3       PARAMETER (MMAX=20)
	      4       DIMENSION X(NDATA),Y(NDATA),SIG(NDATA),ALPHA(NALP,NALP),BETA(MA),
	      5      *    DYDA(MMAX),LISTA(MFIT),A(MA)
