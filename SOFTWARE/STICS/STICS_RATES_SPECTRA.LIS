	   2185 C***************************************************************************************************
	   2186 			IMPLICIT NONE
	   2187       REAL*4 DCR_FSR,			!DCR/FSR
	   2188      .       DCR_RSR ,			!DCR/RSR
	   2189      .       E,				!Energy
	   2190      .       EFF_DATA(0:31,1:178),      !Matrix rate efficiency TCR/Io
	   2191      .       EPQ(0:31),            	!Energy/charge for each voltage step
	   2192      .       ION_MASS(0:21),		!Mass of ions
	   2193      .       M,				!Mass
	   2194      .       MR_ENERGY(0:31,0:21),	!Total energy at each voltage step for rates
	   2195      .       PASSBAND,			!Energy passband of deflection system
	   2196      .       TCR_FSR			!TCR/FSR             
	   2197 
	   2198       INTEGER I,J			!Counters
	   2199 
	   2200       DATA ION_MASS/3.971,11.916,11.916,11.916,15.872,15.872,15.872,20.019,24.112,24.112,27.863,
	   2201      +              27.863,27.863,55.404,55.404,55.404,55.404,55.404,55.404,55.404,1.000,3.971/
	   2202 
	   2203 
	   2204 C		************ STATEMENTS *************
	   2205 
	   2206       DO 10  I = 0,31
	   2207         EPQ(I) = 6.190722 * (1.1225857**I)
	   2208   10  CONTINUE
	   2209 
	   2210       DO 15 J=1,178			!Initialize efficiency array to 1.0
	   2211         DO 14 I=0,31
	   2212           EFF_DATA(I,J) = 1.0
	   2213   14    CONTINUE
