	   3477 C
	   3478       IF (VEL_SWFY .GE. 0) THEN
	   3479       	 PHI_SWF = ACOS(VEL_SWFX/SQRT(VEL_SWFX**2 + VEL_SWFY**2)) * 180.0 / PI
	   3480       ELSE
	   3481       	 PHI_SWF = 360 - (ACOS(VEL_SWFX/SQRT(VEL_SWFX**2 + VEL_SWFY**2)) * 180.0 / PI)
	   3482       ENDIF
	   3483 
	   3484       ANG_NUM = INT(PHI_SWF/22.5 + 0.5)
	   3485       IF ((ANG_NUM .EQ. 0) .OR. (ANG_NUM .GE. 16)) THEN
	   3486         SECT_SWF = 1
	   3487       ELSEIF (ANG_NUM .EQ. 1) THEN
	   3488 	SECT_SWF = 0
	   3489       ELSE
	   3490   	SECT_SWF = 17 - ANG_NUM
	   3491       ENDIF
	   3492 
	   3493       RETURN
	   3494       END


