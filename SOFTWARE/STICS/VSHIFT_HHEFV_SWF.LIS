	   3369      .       EPQ(0:31),            	!Energy/charge for each voltage step
	   3370      .       ION_MASS(1:30),		!Mass of ions
	   3371      .       MR_ENERGY(0:31,1:30)	!Total energy at each voltage step for each ion
