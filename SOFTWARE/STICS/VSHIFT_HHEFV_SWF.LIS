	   3423       SUBROUTINE  CG_TRANS(VEL_SW,V_ION_SP,SECT,TEL_ID, 	!input
	   3424      .				  V_ION_SWF,SECT_SWF)   	!output
	   3425 
	   3426 c The routine calculates the ion speed in the spacecraft frame using:
	   3427 c		V' = V - Vsw       {primed quantities represent solar wind frame)
	   3428 c
	   3429 c 
	   3430 c
	   3431 ccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccccc
	   3432 
	   3433       IMPLICIT NONE
	   3434       REAL*4 PI, SECTOR_SIZE
	   3435       PARAMETER (PI = 3.141592654, SECTOR_SIZE = 22.5)
	   3436 C
	   3437       INTEGER*4  ANG_NUM,
	   3438      .		 I,J,K,L,NN,  			!Array counters
	   3439      .		 SECT,				!Azimuthal Sector spacecraft frame
	   3440      .		 SECT_SWF,
	   3441      .		 TEL_ID				!Telescope 1, 2, 3: polar direction
	   3442 C
	   3443       REAL*4 CGF,				!Compton Getting correction factor
	   3444      .       PHI,
	   3445      . 	     PHI_DEG(0:15),			!Azimuthal angle for given sector
	   3446      .	     PHI_SWF,
	   3447      .       THETA,
	   3448      .	     THETA_DEG(1:3),			!Polar angle for given telescope
	   3449      .	     V_ION_SP,				!Velocity of ion in spacecraft frame
	   3450      .	     V_ION_SWF,				!Velocity of ion in solar wind frame
	   3451      .	     VEL_SW(1:3),			!Solar wind speed in cartesian coordinates
	   3452      .	     VEL_SWFX, VEL_SWFY, VEL_SWFZ	!Cartesian components in SOLAR WIND frame
	   3453 
	   3454       DATA PHI_DEG/22.5, 0.0, 337.5, 315.0, 292.5, 270.0, 247.5, 225.0, 202.5, 180.0, 157.5, 135.0,
	   3455      .             112.5, 90.0, 67.5, 45.0/,
	   3456      .     THETA_DEG/37.0, 90.0,143.0/
	   3457 
	   3458 
	   3459 C
	   3460 C Initialize variables 
	   3461 C
	   3462       THETA = THETA_DEG(TEL_ID) * PI / 180.0
	   3463       PHI   = PHI_DEG(SECT) * PI / 180.0
	   3464 C
	   3465 C Calculate ion velocity in SOLAR WIND frame
	   3466 C
	   3467       VEL_SWFX = V_ION_SP*SIN(THETA)*COS(PHI) - VEL_SW(1)
	   3468       VEL_SWFY = V_ION_SP*SIN(THETA)*SIN(PHI) - VEL_SW(2)
	   3469       VEL_SWFZ = V_ION_SP*COS(THETA)          - VEL_SW(3)
	   3470 	   
	   3471       V_ION_SWF = SQRT( VEL_SWFX**2 + VEL_SWFY**2 + VEL_SWFZ**2 ) 
	   3472 	  
	   3473       CGF = (V_ION_SWF/V_ION_SP)**2 
	   3474 
	   3475 C 
	   3476 C Calculate azimuthal sector in SOLAR WIND frame

