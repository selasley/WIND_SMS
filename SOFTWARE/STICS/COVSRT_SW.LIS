	      1       SUBROUTINE COVSRT(COVAR,NCVM,MA,LISTA,MFIT)
	      2       DIMENSION COVAR(NCVM,NCVM),LISTA(MFIT)
	      3       DO 12 J=1,MA-1
	      4         DO 11 I=J+1,MA
	      5           COVAR(I,J)=0.
	      6 11      CONTINUE
	      7 12    CONTINUE
	      8       DO 14 I=1,MFIT-1
	      9         DO 13 J=I+1,MFIT
	     10           IF(LISTA(J).GT.LISTA(I)) THEN
	     11             COVAR(LISTA(J),LISTA(I))=COVAR(I,J)
	     12           ELSE
	     13             COVAR(LISTA(I),LISTA(J))=COVAR(I,J)
	     14           ENDIF
	     15 13      CONTINUE
	     16 14    CONTINUE
	     17       SWAP=COVAR(1,1)
	     18       DO 15 J=1,MA
	     19         COVAR(1,J)=COVAR(J,J)
	     20         COVAR(J,J)=0.
	     21 15    CONTINUE
	     22       COVAR(LISTA(1),LISTA(1))=SWAP
	     23       DO 16 J=2,MFIT
	     24         COVAR(LISTA(J),LISTA(J))=COVAR(1,J)
	     25 16    CONTINUE
	     26       DO 18 J=2,MA
	     27         DO 17 I=1,J-1
	     28           COVAR(I,J)=COVAR(J,I)
	     29 17      CONTINUE
	     30 18    CONTINUE
	     31       RETURN
	     32       END

