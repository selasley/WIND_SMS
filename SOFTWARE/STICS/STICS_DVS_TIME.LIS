	   2537 	         IF(BASIC_W(I,J,K) .LT. 1.0) THEN		!for bad Basic Rate 
	   2538 		   BASIC_W(I,J,K) = 1.0
	   2539 		   BAD_BR = BAD_BR + 1
	   2540 		   WRITE(56,250)TIME_TO_USE.DOY,TIME_TO_USE.HR,TIME_TO_USE.MIN,TIME_TO_USE.SEC,
	   2541      .			      J,J+1,BASIC_RATE(I,(J+2)/2,K),(BASIC_PHA(I,J,K) + BASIC_PHA(I,J+1,K))
