	   2564 C			****************************************
	   2565 C			*  AVERAGE DATA OVER AZIMUTHAL SECTORS *
	   2566 C			****************************************
	   2567 C Average data over sectors and telescopes
	   2568 C
	   2569 	DO L = 1, 3
	   2570 	  DO I = 0,31
	   2571 	    DO K = 1, 3
	   2572 	      DO J = 0, 15 
	   2573                 OMNI_DATA(I,L) = OMNI_DATA(I,L) + CORR_DATA(I,J,K,L)/16.0/3.0
	   2574                 OMNI_RAW(I,L)  = OMNI_RAW(I,L)  + RAW_COUNTS(I,J,K,L)
	   2575 	      END DO
	   2576 	    END DO
	   2577 	  END DO
	   2578         END DO

