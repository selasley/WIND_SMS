	   2531 	         IF(BASIC_W(I,J,K) .LT. 1.0) THEN		!for bad Basic Rate 
	   2532 C     .		   ( (BASIC_RATE(I,(J+2)/2,K) - (BASIC_PHA(I,J,K) + BASIC_PHA(I,J+1,K))) .GT. 50000.0)) THEN
	   2533 		   BASIC_W(I,J,K) = 1.0
	   2534 		   BAD_BR = BAD_BR + 1
	   2535 		   WRITE(56,250)TIME_TO_USE.DOY,TIME_TO_USE.HR,TIME_TO_USE.MIN,TIME_TO_USE.SEC,
	   2536      .			      J,J+1,BASIC_RATE(I,(J+2)/2,K),(BASIC_PHA(I,J,K) + BASIC_PHA(I,J+1,K))
	   2537  250		   FORMAT(1X,I4.3,3(':',I2.2),3X,'Sectors',I2,'+',I2,'  Basic Rate = ',F7.0,3X,
	   2538      .                       'Basic PHA = ', F7.0)
	   2539 	         ELSE
	   2540 		   GOOD_BR = GOOD_BR + 1
	   2541 	         ENDIF
	   2542 	      ELSE
	   2543 	         BASIC_W(I,J,K) = 1.0
	   2544 	      ENDIF
	   2545               BASIC_W(I,J+1,K) = BASIC_W(I,J,K)
	   2546 	    END DO
	   2547        	  END DO
	   2548         END DO
	   2549 C
	   2550 C Do Basic Rate normalization and efficiency correction. H+, He+2 and He+ are all Basic Rate 2 
	   2551 C
	   2552         DO I = 0, 31
	   2553           IF(DVS_HITS(I).EQ.0) DVS_HITS(I)=1.0
	   2554 	  DO J = 0,15
	   2555 	    DO K = 1,3
	   2556 	      DO L = 1,3
	   2557                 CORR_DATA(I,J,K,L) = RAW_COUNTS(I,J,K,L) * BASIC_W(I,J,2) * DEADTIME_CORR(J)
	   2558      .				   /(EFF_CORR(I,L) * DVS_HITS(I) * GEO_FAC * DELTA_T)
	   2559 	      END DO
	   2560             END DO
	   2561           END DO
	   2562         END DO	
